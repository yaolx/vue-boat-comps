{
  "name": "vue-boat-comps",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite -m test --config ./build/config/base.ts",
    "serve": "vite preview",
    "start:mock": "vite -m mock",
    "start:test": "vite -m test",
    "start:production": "vite -m production",
    "build": "vuedx-typecheck . && vite build",
    "build:test": "vue-tsc --noEmit && vite build -m test",
    "build:production": "vue-tsc --noEmit && vite build -m production",
    "build:tsc": "vue-tsc --noEmit && vite build",
    "build:lib": "vite build -m test --config ./build/config/lib.ts",
    "lint:tsc": "vue-tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "dependencies": {
    "ant-design-vue": "^2.2.8",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "vue": "^3.0.4",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "^3.0.4",
    "@vuedx/typecheck": "^0.3.1-insiders-1606311019.0",
    "@vuedx/typescript-plugin-vue": "^0.3.1-insiders-1606311019.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-plugin-vue": "^8.0.1",
    "husky": "4.3.8",
    "less": "^4.1.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.1.2",
    "vite": "^2.7.0",
    "vite-plugin-components": "^0.13.3",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-mock": "^2.9.6",
    "vitepress": "^0.20.10",
    "vue-eslint-parser": "^8.0.0",
    "vue-tsc": "^0.28.10"
  }
}
